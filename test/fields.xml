<?xml version="1.0" encoding="UTF-8"?>

<p:fields 
  xmlns:p="http://www.gulland.com/software/srdg/1.0/fields" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.gulland.com/software/srdg/1.0/fields fields.xsd ">
  
  <p:field p:classname="com.gulland.fomalhaut.fields.SequentialNumberField">
    <p:name>Sequential Number</p:name>
    <p:description>a sequential number generator</p:description>
    <parameter p:datatype="Double" p:parameterName="start"/>
    <parameter p:datatype="Double" p:parameterName="end"/>
  </p:field>  

  <p:field p:classname="com.gulland.fomalhaut.fields.RandomNumberField">
    <p:name>Random Number</p:name>
    <p:description>a random number generator</p:description>
    <parameter p:datatype="Double" p:parameterName="start"/>
    <parameter p:datatype="Double" p:parameterName="end"/>
  </p:field>

</p:fields>
